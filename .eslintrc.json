{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["vue"],
  "rules": {
    "camelcase": ["error", { "properties": "never", "ignoreDestructuring": true, "allow": ["id_", "_id"] }]
  }
}